Cell[TextData[{
 "In this problem we need to estimate the value of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Integral]", "C"], "zdC"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "245895d3-98ab-426e-b703-6ba066f74e29"],
 " where C is the cylinder determined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["y", "2"]}], "\[LessEqual]", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "babbb086-61e4-492e-b478-b1970217c005"],
 " and 1\[LessEqual]z\[LessEqual]3 and we\[CloseCurlyQuote]re supposed to use \
500 points. We can start off by seeing that this cylinder has a radius of R=1 \
and a height of 2, so it\[CloseCurlyQuote]s volume is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Pi]1", "2"], "*", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "372f13fc-d83b-452f-8eb4-61ac291bd0ea"],
 "=2\[Pi]. Now that we have this we code the rest of it up"
}], "Text",
 CellChangeTimes->{{3.825006932299718*^9, 3.8250071527848444`*^9}, 
   3.8250074384146147`*^9},ExpressionUUID->"85950b6e-09c3-4a8e-8d8e-\
fc58fa567b17"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"monteintegral", "[", "npoints_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "sum", ",", "avg1", ",", "vol1", ",", "x", ",", "y", ",", "z", ",", 
       "coeff"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"x", "=", 
       RowBox[{"RandomReal", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "npoints"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"coeff", "=", 
       RowBox[{"RandomChoice", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "npoints"}], "]"}]}], 
      ";", 
      RowBox[{"(*", 
       RowBox[{
       "This", " ", "will", " ", "make", " ", "something", " ", "randomly", 
        " ", 
        RowBox[{"postive", "/", "negative"}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"y", "=", 
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"coeff", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"Sqrt", "[", 
              RowBox[{"1", "-", 
               RowBox[{
                RowBox[{"x", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], "^", "2"}]}], "]"}]}], "}"}],
            "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "npoints"}], "}"}]}], "]"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"We", " ", "know", " ", "y"}], "\[LessEqual]", 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["x", "2"], " ", "so", " ", "we", " ", "can", " ", 
          "choose", " ", "random", " ", "between", " ", 
          RowBox[{"{", 
           RowBox[{"0", ",", " ", 
            RowBox[{"the", " ", "max", " ", "y", " ", "can", " ", "be"}]}], 
           "}"}], " ", "and", " ", "it", " ", "will", " ", "randomly", " ", 
          "make", " ", "is", " ", "positive", " ", "or", " ", 
          "negative"}]}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"z", "=", 
       RowBox[{"RandomReal", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "3"}], "}"}], ",", "npoints"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"sum", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"npoints", "+", "1"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"sum", "+=", " ", 
         RowBox[{"z", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"avg1", "=", 
       RowBox[{"sum", "/", "npoints"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "avg", " ", "value", " ", "of", " ", "all", " ", "of", " ", "z", " ", 
        "inside", " ", "the", " ", "cylinder"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"vol1", "=", 
       RowBox[{"avg1", "*", "2", "Pi"}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{
       "we", " ", "have", " ", "a", " ", "cylinder", " ", "of", " ", "volume",
         " ", "2", 
        SuperscriptBox["\[Pi]1", "2"]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "vol1", "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.824932597409789*^9, 3.8249327289101124`*^9}, 
   3.824932787939023*^9, {3.824932832427681*^9, 3.8249328395484343`*^9}, 
