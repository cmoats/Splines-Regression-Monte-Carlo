Cell["\<\
In this problem we need to generate 1000 [points in the square [-1,1]x[1,1] \
and find the proportion of them that lie in the unit circle to give an \
estimate of \[Pi]. We can use the fact that we know the area of rectangle is \
4 and multiple this by the ratio of points we find in the unit circle to give \
us the estimate of pi

First we generate 1000 random points in the square and then write the rest of \
the code that does this.\
\>", "Text",
 CellChangeTimes->{{3.8250079528772926`*^9, 3.825008097713728*^9}, {
  3.825008136988483*^9, 
  3.8250081471063614`*^9}},ExpressionUUID->"5fbdb3d9-73f9-4210-80cf-\
519163a1d89c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"points", "=", 
    RowBox[{"RandomReal", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"1000", ",", "2"}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"approxpi", "[", "points_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "incircle", ",", "totalpoints", ",", "x", ",", "y", ",", "estimation", 
      ",", "ratio"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"incircle", "=", "0"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "points", " ", "from", " ", "randomly", " ", 
       "generated", " ", "points", " ", "that", " ", "lie", " ", "in", " ", 
       "the", " ", "unit", " ", "circle"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"totalpoints", "=", 
      RowBox[{"Length", "[", "points", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<", 
        RowBox[{
         RowBox[{"Length", "[", "points", "]"}], "+", "1"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{
          RowBox[{"points", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"y", "=", 
         RowBox[{
          RowBox[{"points", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{
         "Corresponds", " ", "to", " ", "x", " ", "and", " ", "y", " ", 
          "data", " ", "points"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"x", "^", "2"}], "+", 
            RowBox[{"y", "^", "2"}]}], "\[LessEqual]", "1"}], ",", 
          RowBox[{"incircle", "+=", "1"}]}], "]"}]}]}], "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Checks", " ", "to", " ", "see", " ", "if", " ", "our", " ", "points", 
       " ", "are", " ", "inside", " ", "the", " ", "unit", " ", "circle"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"ratio", "=", 
      RowBox[{"incircle", "/", "totalpoints"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "divides", " ", "the", " ", "total", " ", "number", " ", "of", " ", 
       "points", " ", "in", " ", "the", " ", "unit", " ", "circle", " ", "by",
        " ", "1000"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"estimation", "=", 
      RowBox[{"4", "*", "ratio"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Multiplies", " ", "the", " ", "ratio", " ", "of", " ", "points", " ", 
       "inside", " ", "the", " ", "unit", " ", "circle", " ", "by", " ", 
       "the", " ", "area", " ", "of", " ", "the", " ", "square"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"N", "[", "estimation", "]"}], "]"}]}]}], 
   "]"}]}], "\[IndentingNewLine]"}], "Input",
