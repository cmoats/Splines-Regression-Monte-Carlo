  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "<", "11"}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{"AppendTo", "[", 
    RowBox[{"distances", ",", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"datay", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "-", 
         RowBox[{"reg4", "[", 
          RowBox[{"datax", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "]"}]}], ")"}], "^", "2"}], 
      "]"}]}], "]"}]}], "]"}]], "Input",
